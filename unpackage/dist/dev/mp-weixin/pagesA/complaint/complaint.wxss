page {
  height: 100%;
  width: 100%;
}
.complaint {
  width: 100%;
  height: 100%;
  background-color: #F2F2F2;
}
.complaint .complaint_main {
  width: 620rpx;
  background-color: #fff;
  border-radius: 15rpx;
  padding: 0px 35rpx;
  padding-bottom: 30rpx;
}
.complaint .complaint_main .complaint_main_title {
  color: #666666;
  font-size: 26rpx;
  padding: 30rpx 0px;
  font-weight: bold;
}
.complaint .complaint_main .complaint_main_type {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #9A9A9A;
}
.complaint .complaint_main .complaint_main_type .complaint_main_typeList {
  color: #909090;
  font-size: 26rpx;
  border: 1rpx solid #909090;
  border-radius: 5rpx;
  padding: 10rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.complaint .complaint_main .complaint_main_type .active {
  background-color: #DD524D;
  color: #fff;
  border: 0;
}
.complaint .complaint_main .complaint_main_textarea {
  width: 618rpx;
  height: 200rpx;
  border: 1rpx solid #9A9A9A;
  border-radius: 10rpx;
}
.complaint .complaint_btn {
  width: 690rpx;
  height: 80rpx;
  background-color: #0CC064;
  margin: 80rpx auto;
  text-align: center;
  color: #fff;
  line-height: 80rpx;
  font-size: 30rpx;
  border-radius: 10rpx;
}
.complaint .isOk {
  background-color: #ccc;
}

