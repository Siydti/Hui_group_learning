<view class="settlementPost"><view class="infos"><view class="li"><view class="left"><text>机构名称：</text><input type="text" placeholder="请输入机构名称" data-event-opts="{{[['input',[['inputtitle',['$event']]]]]}}" value="{{title}}" bindinput="__e"/></view></view><view class="li"><view class="left"><text>真实姓名：</text><input type="text" placeholder="请输入真实姓名" data-event-opts="{{[['input',[['inputname',['$event']]]]]}}" value="{{name}}" bindinput="__e"/></view></view><view class="li"><view class="left"><text>联系电话：</text><input type="number" maxlength="11" placeholder="请输入您的联系电话" data-event-opts="{{[['input',[['inputphone',['$event']]]]]}}" value="{{phone}}" bindinput="__e"/></view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn" bindtap="__e">提交</view></view>