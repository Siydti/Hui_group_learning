page {
  width: 100%;
  min-height: 100%;
  background: #F2F2F2;
}
.browse .ul .li {
  background: #FFFFFF;
  padding: 30rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 0;
  margin-top: 4rpx;
}
.browse .ul .li image {
  width: 700rpx;
  height: 220rpx;
}
.browse .ul .li .r {
  margin-left: 20rpx;
}
.browse .ul .li .r .name {
  font-size: 30rpx;
  color: #333333;
  font-weight: bold;
}
.browse .ul .li .r .info {
  font-size: 26rpx;
  color: #999999;
  padding-top: 30rpx;
}

